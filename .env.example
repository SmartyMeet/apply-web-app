# Candidate Apply Portal - Environment Variables
# Copy this file to .env.local and update values as needed

# Optional: Default tenant name (default: "default")
NEXT_PUBLIC_DEFAULT_TENANT=default

# Optional: CDN base URL for theme/branding (default: https://cdn.smartytalent.eu)
NEXT_PUBLIC_THEME_BASE_URL=https://cdn.smartytalent.eu

# Optional: CDN base URL for tenant logos
# Logo URL pattern: {NEXT_PUBLIC_CDN_BASE_URL}/tenant/{tenantName}/logo.jpeg
NEXT_PUBLIC_CDN_BASE_URL=https://cdn.test-smartytalent.eu

# Environment name used for EventBridge bus naming (default: "dev")
SM_ENV=dev03
